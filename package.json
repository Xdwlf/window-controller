{
  "name": "dashboard-ui",
  "version": "1.0.0",
  "private": true,
  "description": "Window Controller UI",
  "homepage": "https://bitbucket.org/reviewerdashboard/dashboard-ui#readme",
  "scripts": {
    "preinstall": "npm run setup",
    "setup": "node ./scripts/setup",
    "start": "npm run start-client",
    "test": "jest --verbose",
    "coverage": "jest --coverage",
    "test-silent": "jest --silent",
    "start-client": "cross-env NODE_ENV=development webpack-dev-server --open --config config/webpack.dev.js ",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js --mode production"
  },
